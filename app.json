{
  "expo": {
    "name": "AI Experience",
    "slug": "ai-experience",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aiexperience.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.aiexperience.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Uygulama fotoğraf çekmek için kamera erişimi gerektirir."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Uygulama galeriden fotoğraf seçmek için erişim gerektirir."
        }
      ]
    ],
    "scheme": "aiexperience",
    "extra": {
      "apiBaseUrl": "http://127.0.0.1:8000",
      "useMockAi": true
    }
  }
}
